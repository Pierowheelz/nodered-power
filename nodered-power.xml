<?xml version="1.0" encoding="utf-8"?>
<Container>
<Beta>True</Beta>
<Category>HomeAutomation:</Category>
<Date>2020-08-21</Date>
<Changes>
[b]21.Aug.2020:[/b]Added[br]
</Changes>
<Support>http://forums.unraid.net</Support>
<Name>NoderedPower</Name>
<Description>
Nodered power control for Server.
</Description>
<Registry>https://hub.docker.com/r/pierowheelz/nodered-power</Registry>
<GitHub>https://github.com/Pierowheelz/nodered-power</GitHub>
<Repository>pierowheelz/nodered-power</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
  <Mode>bridge</Mode>
  <Publish>
    <Port>
      <HostPort>8200</HostPort>
      <ContainerPort>8200</ContainerPort>
      <Protocol>tcp</Protocol>
    </Port>
  </Publish>
</Networking>
<Data>
  <Volume>
    <HostDir>/usr/src/nodered-power</HostDir>
    <ContainerDir>/usr/src/nodered-power</ContainerDir>
    <Mode>rw</Mode>
  </Volume>
  <Volume>
    <HostDir>/mnt/user/appdata/scripts</HostDir>
    <ContainerDir>/mnt/user/appdata/scripts</ContainerDir>
    <Mode>rw</Mode>
  </Volume>
</Data>
<WebUI>http://[IP]:[PORT:8200]</WebUI>
<Banner>http://i.imgur.com/zXpacAF.png</Banner>
<Icon>http://i.imgur.com/zXpacAF.png</Icon>
<TemplateURL>https://raw.githubusercontent.com/pierowheelz/nodered-power/master/nodered-power.xml</TemplateURL>
</Container>
